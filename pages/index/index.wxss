/**index.wxss**/
.head{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 80rpx;
  background: #ffffff;
  border-top: 1px solid #eee
}

.up_page{
  float: left;
  font-size: 26rpx;
  height: 80rpx;
  line-height: 80rpx;
  margin-left: 201rpx;
  margin-right: 20rpx;
}

.jump_page{
  width: 80rpx;
  height: 60rpx;
  float: left;
  background: #eee;
  margin-top: 10rpx;
  text-align: center;
  border-radius: 20rpx;
  font-size: 30rpx;
}

.next_page,.jump_action{
  float: left;
  font-size: 26rpx;
  height: 80rpx;
  line-height: 80rpx;
  margin-left: 20rpx;
}

.content{
  margin-left: 15rpx;
  margin-right: 15rpx;
  margin-top: 100rpx;
}

.list{
  float: left;
  width: 330rpx;
  height: 330rpx;
  background: #ffffff;
  margin-left: 15rpx;
  margin-right: 15rpx;
  margin-bottom: 30rpx;
  border-radius: 20rpx;
  text-align: center;
}

.list_img{
  width: 164rpx;
  height: 127rpx;
  margin-top: 80rpx
}

.list_txt{
  display: block;
  font-size: 28rpx;
  margin-top: 20rpx
}